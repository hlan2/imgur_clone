<h1>Add a picture</h1>
<%= form_tag "/pictures" do %>
  <div>
  	<%= label_tag :artist%><br>
  	<%= text_field_tag :artist%>
  </div> 
  <p>
  	<%= label_tag :title%><br>
  	<%= text_field_tag :title%>
  </p>
  <p>
  	<%= label_tag :url%><br>
  	<%= text_field_tag :url%>
  </p>
  <p>
  <%= submit_tag "Save" %>
  </p>
  <% end %>